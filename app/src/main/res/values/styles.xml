<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>




    </style>

 <style name="AppThemeNextGen" parent="AppTheme">

 </style>

    <style name="BotaoSalvar" parent="Widget.AppCompat.Button.Colored">

        <!--<item name="android:backgroundTint"></item>-->


    </style>

    <style name="BotaoGeral" parent="Widget.AppCompat.Button.Colored">

        <item name="android:backgroundTint">@color/holo_blue_dark</item>
        <item name="android:textColor">@color/blue_click_old</item>


    </style>
    <style name="BotaoAzul" parent="Widget.AppCompat.Button.Colored">

        <item name="android:backgroundTint">@color/blue_click_old</item>
        <item name="android:textColor">@color/white</item>


    </style>
    <style name="BotaoTelaPrincipal" parent="Widget.AppCompat.Button.Colored">

        <item name="android:backgroundTint">@color/blue_twitter</item>
        <item name="android:textColor">@color/colorBlack</item>


    </style>

    <style name="ListaPainelAlunos" parent="Widget.AppCompat.Button.Colored">

        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
        <item name="android:divider">@color/green_light</item>
        <item name="android:dividerHeight">1dp</item>
    </style>

<!--Tabela division color-->
    <style name="Divider">
        <item name="android:layout_width">1dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/green_light</item>
    </style>

    <style name="Divider_invisible">
        <item name="android:layout_width">1dip</item>
        <item name="android:layout_height">match_parent</item>
    </style>



</resources>
