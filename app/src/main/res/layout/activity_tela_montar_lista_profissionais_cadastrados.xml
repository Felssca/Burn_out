<?xml version="1.0" encoding="utf-8"?>


<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:fillViewport="true"
    tools:context=".activity.TelaMontarDadosCompletosActivity">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:text="@string/titulo_ver_todos_dados"
            android:textAllCaps="true"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="16dp"
                app:cardElevation="12dp"
                app:cardMaxElevation="18dp">

                <LinearLayout
                    android:id="@+id/edit_resistration_group_personal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/blue_twitter"
                    android:minHeight="60dp">


                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="15dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/ic_all"
                        />

                    <View
                        android:layout_width="2px"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="15dp"
                        android:background="@color/white_title"
                        />

                    <TextView
                        android:layout_width="232dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="15dp"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/titulo_lista_ver_todos_dados"
                        android:textColor="@color/white_title"
                        android:textSize="18sp" />

                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="16dp"
                app:cardElevation="12dp"
                app:cardMaxElevation="18dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    android:orientation="vertical">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="30dp"
                        android:orientation="vertical">
                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/lbl_txt_titulo_cadastro_anamnese"
                            android:textColor="@color/green_light"
                            />
                        <TextView
                            android:id="@+id/lista_dados_anamnase_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            /><TextView
                            android:id="@+id/lista_dados_anamnase_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            /><TextView
                            android:id="@+id/lista_dados_anamnase_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            /><TextView
                            android:id="@+id/lista_dados_anamnase_4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            /><TextView
                            android:id="@+id/lista_dados_anamnase_5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            /><TextView
                            android:id="@+id/lista_dados_anamnase_6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            /><TextView
                            android:id="@+id/lista_dados_anamnase_7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            /><TextView
                            android:id="@+id/lista_dados_anamnase_8"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            /><TextView
                            android:id="@+id/lista_dados_anamnase_9"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />
                        <TextView
                            android:id="@+id/lista_dados_anamnase_10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_11"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_12"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_13"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_15"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_17"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_18"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_19"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_21"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />   <TextView
                            android:id="@+id/lista_dados_anamnase_22"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            />


                    </LinearLayout>






                </LinearLayout>
            </android.support.v7.widget.CardView>


        </LinearLayout>



    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
